@{
    ViewData["Title"] = "Home Page";
}

<h1 class="display-4">Header</h1>
<h2>Subheader</h2>
<div class="row">
    <div class="col">
        <button type="button" class="btn btn-secondary" onclick="clearNumbers()">Clear numbers</button>
        <button type="button" class="btn btn-secondary" onclick="addNumber()">Add number</button>
        <button type="button" class="btn btn-secondary" onclick="sumNumbers()">Sum Numbers</button>
    </div>
</div>
<div class="row mt-3">
    <div id="nums" class="col"></div>
</div>
<h6 id="count" class="mt-3">Count: </h6>
<h6 id="sum">Sum: Not summed</h6>
